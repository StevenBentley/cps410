<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/parkinglot_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/backgroundColor"
    android:orientation="vertical"
    tools:context=".MainActivity">

    <include
        android:id="@+id/include"
        layout="@layout/drawer_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />


    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/backgroundd"
        android:fitsSystemWindows="true"
        app:elevation="0dip"
        app:layout_constraintTop_toTopOf="parent"
        app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarAdd"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:title="@string/header_parking_lot1"
            app:titleTextAppearance="@style/HeaderText"/>

        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
            android:id="@+id/pull_refresh"
            android:layout_width="388dp"
            android:layout_height="427dp"
            android:layout_gravity="center">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:background="@drawable/demo1">

                <ImageView
                    android:id="@+id/s0taken"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_taken"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.091"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.215"
                    app:srcCompat="@drawable/circleclosed" />

                <ImageView
                    android:id="@+id/s0open"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_open"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.091"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.215"
                    app:srcCompat="@drawable/circleopen" />

                <ImageView
                    android:id="@+id/s1open"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_open"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.216"
                    app:srcCompat="@drawable/circleopen" />

                <ImageView
                    android:id="@+id/s1taken"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_taken"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.216"
                    app:srcCompat="@drawable/circleclosed" />


                <ImageView
                    android:id="@+id/s2open"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_open"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.897"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.215"
                    app:srcCompat="@drawable/circleopen" />

                <ImageView
                    android:id="@+id/s2taken"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_taken"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.897"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.215"
                    app:srcCompat="@drawable/circleclosed" />

                <ImageView
                    android:id="@+id/s3taken"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_taken"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.091"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.79"
                    app:srcCompat="@drawable/circleclosed" />

                <ImageView
                    android:id="@+id/s3open"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_open"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.091"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.79"
                    app:srcCompat="@drawable/circleopen" />

                <ImageView
                    android:id="@+id/s4taken"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_taken"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.79"
                    app:srcCompat="@drawable/circleclosed" />

                <ImageView
                    android:id="@+id/s4open"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_open"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.498"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.79"
                    app:srcCompat="@drawable/circleopen" />


                <ImageView
                    android:id="@+id/s5taken"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_taken"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.897"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.79"
                    app:srcCompat="@drawable/circleclosed" />

                <ImageView
                    android:id="@+id/s5open"
                    android:layout_width="60dp"
                    android:layout_height="60dp"
                    android:contentDescription="@string/spot_open"
                    android:visibility="invisible"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="0.897"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.79"
                    app:srcCompat="@drawable/circleopen" />


            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
    </com.google.android.material.appbar.AppBarLayout>

    <com.google.android.material.navigation.NavigationView
        android:id="@+id/navigation_view"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="@color/backgroundColor"
        android:fitsSystemWindows="true"
        app:headerLayout="@layout/drawer_header"
        app:layout_constraintStart_toStartOf="@+id/include"
        app:menu="@menu/menu_drawer"
        app:itemTextAppearance="@style/menu_text_style"
        android:theme="@style/NavigationTheme"
        />

</androidx.drawerlayout.widget.DrawerLayout>


